{
  "version": 2,
  "projects": {
    "billing": {
      "projectType": "application",
      "root": "apps/billing",
      "sourceRoot": "apps/billing/src",
      "prefix": "mf-app",
      "targets": {
        "build": {
          "executor": "@nrwl/angular:webpack-browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/billing",
            "index": "apps/billing/src/index.html",
            "main": "apps/billing/src/main.ts",
            "polyfills": "apps/billing/src/polyfills.ts",
            "tsConfig": "apps/billing/tsconfig.app.json",
            "assets": [
              "apps/billing/src/favicon.ico",
              "apps/billing/src/assets"
            ],
            "styles": ["apps/billing/src/styles.css"],
            "scripts": [],
            "customWebpackConfig": {
              "path": "apps/billing/webpack.config.js"
            }
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/billing/src/environments/environment.ts",
                  "with": "apps/billing/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all",
              "customWebpackConfig": {
                "path": "apps/billing/webpack.prod.config.js"
              }
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "executor": "@nrwl/angular:webpack-server",
          "port": 3001,
          "configurations": {
            "production": {
              "browserTarget": "billing:build:production"
            },
            "development": {
              "browserTarget": "billing:build:development",
              "port": 3001
            }
          },
          "defaultConfiguration": "development",
          "options": {}
        },
        "extract-i18n": {
          "executor": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "billing:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/billing/src/**/*.ts",
              "apps/billing/src/**/*.html"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/billing"],
          "options": {
            "jestConfig": "apps/billing/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "billing-e2e": {
      "root": "apps/billing-e2e",
      "sourceRoot": "apps/billing-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/billing-e2e/cypress.json",
            "devServerTarget": "billing:serve:development"
          },
          "configurations": {
            "production": {
              "devServerTarget": "billing:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/billing-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
